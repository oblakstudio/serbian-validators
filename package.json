{
  "name": "serbian-validators",
  "version": "1.0.1",
  "license": "GPL-3.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    {
      "name": "Sibin Grasic",
      "email": "sibin.grasic@oblak.studio",
      "url": "https://oblak.studio"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oblakstudio/serbian-validators.git"
  },
  "bugs": "https://github.com/oblakstudio/serbian-validators/issues",
  "keywords": [],
  "scripts": {
    "build": "rimraf dist && tsc",
    "watch": "rimraf dist && tsc --watch",
    "build:production": "rimraf dist && tsc --project tsconfig.build.json",
    "lint": "eslint lib/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": ">= 14"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^10.0.4",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.1.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
